#ques 4
monthsL=['jan','feb','march','may']
monthsT=('jan','feb','march','may')
#part a
monthsL.insert(3,'april')
print(monthsL)
monthsT.insert(3,'april')
print(monthsT)

#part b
monthsL.append('june')
print(monthsL)
monthsT.append('june')
print(monthsT)

#part c
monthsL.pop()
print(monthsL)
monthsT.pop()
print(monthsT)

#part d
monthsL.pop(1)
print(monthsL)
monthsT.pop(1)
print(monthsT)

#part e
monthsL.reverse()
print(monthsL)
monthsT.reverse()
print(monthsT)

#part f
monthsL.sort()
print(monthsL)
monthsT.sort()
print(monthsT)
