+lst=[-1,2,-3,4,5,6,0,8,10]
#part (a)
if len(lst)%2==0:
    x=len(lst)/2
    print("the middle term is",x)
    print("the index of the middle element is")
    print(lst.index(x))
else:
    y=int(len(lst)/2+2)
    print("the middle term is",y)
    print("the index of the middle element is")
    print(lst.index(y))
#part c
lst=[-1,-3,4,5,6,8,10]
lst.sort(reverse=True)
print(lst)


#part d
lst=[-1,2,-3,4,5,6,0,8,5,10]
print(lst)
x=lst.pop(0)
lst.append(x)
print(lst)




