#ques1
sample=[(2,3),(4,7),(8,11),(3,6)]
x=max(sample)
y=min(sample)
print("the maximum value is",x)
print("the minimum value is",y)

#ques 2
import math
for i in range(4):
    x=int(input("enter x co-ordinate:"))
    y=int(input("enter y co-ordinate:"))
    print(x,y)
    z=math.sqrt(x**2+y**2)
    if z<10:
        print(True)
    else:
        print(False)

